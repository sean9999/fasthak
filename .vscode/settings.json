{
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/.hg/store/**": true,
        ".flatpak/**": true,
        "_build/**": true
    },
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#3723bc",
        "activityBar.activeBorder": "#2f7c47",
        "activityBar.background": "#00000000",
        "activityBar.foreground": "#ffffff99",
        "activityBar.inactiveForeground": "#ffffff66",
        "activityBarBadge.background": "#422c74",
        "activityBarBadge.foreground": "#e7e7e7",
        "statusBar.background": "#ff770099",
        "statusBar.foreground": "#ffffff99",
        "statusBarItem.hoverBackground": "#ffffff",
        "titleBar.activeBackground": "#084d1e",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#21573299",
        "titleBar.inactiveForeground": "#e7e7e799",
        "sash.hoverBorder": "#2f7c47",
        "statusBarItem.remoteBackground": "#3723bc",
        "statusBarItem.remoteForeground": "#ffffff99",
        "commandCenter.border": "#e7e7e799",
        "scrollbarSlider.activeBackground": "#ff770099",
        "editor.hoverHighlightBackground": "#ff770099"
